        </Field>
        <Field Name="RegistrationDTTM">
          <DataField>RegistrationDTTM</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT	PA.PatientNumber,
			PA.Name,
			PA.Age,   
			PA.RegistrationDTTM,
			isNull(PAD.Add1,'') + isNull(PAD.Add2,'') as [Address]
			--IPA.PrimaryPhysicianID  Consultant,
			--InPD.DateTimeofDischarge,  
			--BB.BedID
	FROM	PatientVisit PV INNER JOIN Patient PA ON PV.PatientID = PA.PatientID
			INNER JOIN PatientAddress PAD on PA.PatientID = PAD.PatientID  
			--INNER JOIN InPatientAdmissionDetails IPA ON IPA.VisitID = PV.ParentVisitId
			--INNER JOIN InPatientDischargeDetails InPD on PV.PatientVisitId=InPD.VisitID
			--INNER JOIN BedBooking BB on PV.PatientVisitId =BB.VisitID
	WHERE	PAD.AddressType = 'P' AND PV.PatientVisitId = @pVisitID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@pVisitID">
            <Value>=Parameters!pVisitID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
